\" data-id=\"6517e4e2\" data-element_type=\"widget\" data-widget_type=\"text-editor.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\t\t\t\t<div id=\"ChartIndividual\"><\/div>\n<div class=\"my_text\"><\/div>\n<script>\n    window.Promise || document.write(\n          '<script src=\"https:\/\/cdn.jsdelivr.net\/npm\/promise-polyfill@8\/dist\/polyfill.min.js\" async><\\\/script>'\n        )\n  \twindow.Promise || document.write(\n      '<script src=\"https:\/\/cdn.jsdelivr.net\/npm\/eligrey-classlist-js-polyfill@1.2.20171210\/classList.min.js\" async><\\\/script>'\n    )\n    window.Promise || document.write(\n      '<script src=\"https:\/\/cdn.jsdelivr.net\/npm\/findindex_polyfill_mdn\" async><\\\/script>'\n    )\n<\/script>\n<script src=\"\/wp-content\/themes\/astra-child\/js\/apexcharts.min.js\"><\/script>\n<script src=\"\/wp-content\/themes\/astra-child\/js\/fetch.js\"><\/script>\n<script src=\"\/wp-content\/themes\/astra-child\/js\/update.js\"><\/script>\n<script src=\"\/wp-content\/themes\/astra-child\/js\/utilities.js\"><\/script>\n<script src=\"\/wp-content\/themes\/astra-child\/js\/render.js\"><\/script>\n<script src=\"\/wp-content\/themes\/astra-child\/js\/chartloader-individual.js\"><\/script>\t\t\t\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-5bc397e elementor-widget elementor-widget-html\" data-id=\"5bc397e\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<div>\n    <button id=\"date-back\">Previous Day<\/button>\n    <span id=\"current-date\"><\/span>\n    <button id=\"date-forward\">Next Day<\/button>\n<\/div>\n<script>\ndocument.addEventListener(\"DOMContentLoaded\", function() {\n    \/\/ Initialize date to today's date or from URL if available\n    var currentDate = new Date(); \/\/ Start with today's date\n    console.log(`Current day: ${currentDate.getDay()} Current Month: ${currentDate.getMonth()} Time: ${currentDate.getHours()}:${currentDate.getMinutes()}`)\n\n    const urlParams = new URLSearchParams(window.location.search);\n    const dateParam = urlParams.get('date');\n    if (dateParam) {\n        currentDate = new Date(dateParam);\n    }\n\n    updateDateDisplay();\n\n    \/\/ Event listener for the Back button\n    document.getElementById(\"back-button\").addEventListener(\"click\", function() {\n        window.location.href = '\/search';\n    });\n\n    \/\/ Event listener for the Previous Day button\n    document.getElementById(\"date-back\").addEventListener(\"click\", function() {\n        currentDate.setDate(currentDate.getDate() - 1);\n        updateDateDisplay();\n        updateUrlDateParam();\n        location.reload(); \/\/ Reload data based on new date\n    });\n\n    \/\/ Event listener for the Next Day button\n    document.getElementById(\"date-forward\").addEventListener(\"click\", function() {\n        currentDate.setDate(currentDate.getDate() + 1);\n        if (currentDate <= new Date()) { \/\/ Prevent future dates\n            updateDateDisplay();\n            updateUrlDateParam();\n            location.reload(); \/\/ Reload data based on new date\n        } else {\n            currentDate.setDate(currentDate.getDate() - 1); \/\/ Revert date change\n        }\n    });\n\n    \/\/ Function to update date display and hide\/show next day button\n    function updateDateDisplay() {\n        var todayDate = new Date(); \/\/ Get today's date\n        var dateString = currentDate.toISOString().split('T')[0];\n        document.getElementById(\"current-date\").textContent = dateString;\n\n        \/\/ Hide next day button if currentDate is today's date\n        const dateForwardButton = document.getElementById(\"date-forward\");\n        if (currentDate.toDateString() === todayDate.toDateString()) {\n            dateForwardButton.style.display = \"none\";\n        } else {\n            dateForwardButton.style.display = \"inline-block\"; \/\/ Show the button if not today\n        }\n    }\n\n    \/\/ Function to update URL with current date parameter\n    function updateUrlDateParam() {\n        var dateString = currentDate.toISOString().split('T')[0];\n        const url = new URL(window.location.href);\n        url.searchParams.set('date', dateString);\n        window.history.pushState({ path: url.href }, '', url.href); \/\/ Update URL without reloading\n    }\n\n    \/\/ Function to fetch chart data based on parameters\n    async function fetchChartData(url) {\n        try {\n            const response = await fetch(url);\n            if (!response.ok) {\n                throw new Error(`HTTP error! status: ${response.status}`);\n            }\n            return response.json();\n        } catch (error) {\n            console.error('Failed to fetch chart data:', error);\n            return null;\n        }\n    }\n\n    \/\/ Function to get query parameters from URL\n    function getQueryParams() {\n        const queryParams = new URLSearchParams(window.location.search);\n        return {\n            asset: queryParams.get('asset'),\n            chartType: queryParams.get('chartType'),\n            userDate: queryParams.get('date'), \/\/ Expecting format YYYY-MM-DD\n        };\n    }\n\n    \/\/ Function to filter data based on user date\n    function filterResponseData(jsonData, userDate) {\n        const userDateTime = new Date(userDate).getUTCTime();\n        const oneDay = 24 * 60 * 60 * 1000; \/\/ Milliseconds in one day\n\n        return jsonData.find(item => {\n            const itemStartDateTime = new Date(item.start_date).getTime();\n            const itemEndDateTime = new Date(item.end_date).getTime();\n\n            \/\/ Check if userDate is between (itemStartDateTime - 1 day) and (itemEndDateTime + 1 day)\n            return userDateTime >= (itemEndDateTime - oneDay) && userDateTime <= (itemEndDateTime + oneDay);\n        });\n    }\n\n    \/\/ Function to handle page load and fetch data\n    async function handlePageLoad() {\n        const { asset, chartType, userDate } = getQueryParams();\n        const chartTypeMapping = {\n    \"Moving_Average\": \"Moving_Averages_responses\"\n};\n\n    \/\/ Check if there's a special mapping for the chartType\n    const mappedChartType = chartTypeMapping[chartType] || chartType;\n\n        \/\/ Use today's date if userDate is not provided or invalid\n        const currentDateISOString = currentDate.toISOString().split('T')[0];\n        const effectiveDate = userDate || currentDateISOString;\n\n        if (asset && chartType) {\n            const userDateTime = new Date(new Date(effectiveDate).getTime() + 21600000);\n            printInDiv('assetname', asset);\n\t\t    printInDiv('dateQuery', userDateTime.toDateString());\n\t\t    printInDiv('chartType', chartType.replaceAll(\"_\", \" \"));\n\t\t    document.querySelector('title').textContent = `${chartType.replaceAll(\"_\", \" \")} Chart for ${asset} on ${userDateTime.toDateString()}`;\n            const urls = [\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Average_Price_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Bollinger_Bands_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Hikkake_Pattern_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Stochastic_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Fibonacci_Retracement_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Parabolic_SAR_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Chaikin_AD_Oscillator_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Average_Directional_Movement_Index_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Moving_Averages_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=On_Balance_Volume_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Ultimate_Oscillator_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Relative_Strength_Index_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=McClellan_Oscillator_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=Coppock_Indicator_responses`,\n                `https:\/\/dwcf.gny.io\/get\/ChatGPT_AllMarket\/${asset}USDT\/allmarkets\/1d?latest=360&indicator=MACD_responses`\n            ];\n\n            const selectedUrl = urls.find(url => url.includes(mappedChartType));\n            const jsonData = await fetchChartData(selectedUrl);\n            if (jsonData) {\n                const filteredData = filterResponseData(jsonData, effectiveDate);\n                if (filteredData) {\n                    displayData(filteredData);\n                } else {\n                    console.log('No data found for the specified date');\n                }\n            }\n        } else {\n            console.log(\"Missing URL parameters or invalid. Using defaults.\");\n            \/\/ Here you can handle what to display or do when parameters are missing or invalid\n        }\n    }\n\n    \/\/ Function to display data in tooltip\n    function displayData(data) {\n        const tooltipDiv = document.querySelector('.tooltip');\n        tooltipDiv.innerHTML = `\n            <p>Generated: ${new Date(data._id).toISOString().split('T')[0]}<\/p>\n            <p>Period: ${new Date(data.start_date).toISOString().split('T')[0]} to ${new Date(data.end_date).toISOString().split('T')[0]}<\/p>\n            <p>${data.response}<\/p> <!-- Display response without \"Response:\" label -->\n        `;\n    }\n\n    handlePageLoad(); \/\/ Initial page load\n});\n<\/script>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-2581333 e-flex e-con-boxed e-con e-parent\" data-id=\"2581333\" data-element_type=\"container\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t\t\t<div class=\"elementor-element elementor-element-b536b5f elementor-widget elementor-widget-html\" data-id=\"b536b5f\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<div class=\"tooltip\"><\/div>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>Chart for on New Search Previous Day Next Day<\/p>\n","protected":false},"author":1,"featured_media":0,"parent":0,"menu_order":0,"comment_status":"closed","ping_status":"closed","template":"elementor_header_footer","meta":{"_acf_changed":false,"site-sidebar-layout":"default","site-content-layout":"","ast-site-content-layout":"default","site-content-style":"default","site-sidebar-style":"default","ast-global-header-display":"","ast-banner-title-visibility":"","ast-main-header-display":"","ast-hfb-above-header-display":"","ast-hfb-below-header-display":"","ast-hfb-mobile-header-display":"","site-post-title":"","ast-breadcrumbs-content":"","ast-featured-img":"","footer-sml-layout":"","theme-transparent-header-meta":"default","adv-header-id-meta":"","stick-header-meta":"","header-above-stick-meta":"","header-main-stick-meta":"",